<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="BPEL_Tarea4"
    targetNamespace="http://enterprise.netbeans.org/bpel/BPEL_Tarea4/BPEL_Tarea4"
    xmlns:tns="http://enterprise.netbeans.org/bpel/BPEL_Tarea4/BPEL_Tarea4"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:ns0="http://ServicioCliente.org/" xmlns:ns1="http://ServicioRastreo.org/" xmlns:ns2="http://ServicioPiloto.org/">
    <import namespace="http://j2ee.netbeans.org/wsdl/BPEL_Tarea4/src/PeticionCLiente" location="PeticionCLiente.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/Service_ClienteWrapper" location="Service_ClienteWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://ServicioCliente.org/" location="file:/C:/Users/eljul/Documents/GitHub/SA_Tarea2/Referencias/ServidorCliente/Service_Cliente.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/Service_PilotoWrapper" location="Service_PilotoWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://ServicioPiloto.org/" location="file:/C:/Users/eljul/Documents/GitHub/SA_Tarea2/Referencias/ServidorPiloto/Service_Piloto.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/Service_RastreoWrapper" location="Service_RastreoWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://ServicioRastreo.org/" location="file:/C:/Users/eljul/Documents/GitHub/SA_Tarea2/Referencias/ServidorRastreo/Service_Rastreo.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="LinkCliente" xmlns:tns="http://enterprise.netbeans.org/bpel/Service_ClienteWrapper" partnerLinkType="tns:Service_ClienteSoapLinkType" partnerRole="Service_ClienteSoapRole"/>
        <partnerLink name="LinkPiloto" xmlns:tns="http://enterprise.netbeans.org/bpel/Service_PilotoWrapper" partnerLinkType="tns:Service_PilotoSoapLinkType" partnerRole="Service_PilotoSoapRole"/>
        <partnerLink name="LinkRastreo" xmlns:tns="http://enterprise.netbeans.org/bpel/Service_RastreoWrapper" partnerLinkType="tns:Service_RastreoSoapLinkType" partnerRole="Service_RastreoSoapRole"/>
        <partnerLink name="LinkPeticionCliente" xmlns:tns="http://j2ee.netbeans.org/wsdl/BPEL_Tarea4/src/PeticionCLiente" partnerLinkType="tns:PeticionCLiente" myRole="PeticionCLientePortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="ObtenerConductoresDisponiblesOut" xmlns:tns="http://ServicioPiloto.org/" messageType="tns:ObtenerConductoresDisponiblesSoapOut"/>
        <variable name="ObtenerConductoresDisponiblesIn" xmlns:tns="http://ServicioPiloto.org/" messageType="tns:ObtenerConductoresDisponiblesSoapIn"/>
        <variable name="ObtenerPropuestaPilotoOut" xmlns:tns="http://ServicioRastreo.org/" messageType="tns:ObtenerPropuestaPilotoSoapOut"/>
        <variable name="ObtenerPropuestaPilotoIn" xmlns:tns="http://ServicioRastreo.org/" messageType="tns:ObtenerPropuestaPilotoSoapIn"/>
        <variable name="PeticionIniOut" xmlns:tns="http://ServicioCliente.org/" messageType="tns:PeticionIniSoapOut"/>
        <variable name="PeticionIniIn" xmlns:tns="http://ServicioCliente.org/" messageType="tns:PeticionIniSoapIn"/>
        <variable name="PeticionCLienteOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/BPEL_Tarea4/src/PeticionCLiente" messageType="tns:PeticionCLienteOperationResponse"/>
        <variable name="PeticionCLienteOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/BPEL_Tarea4/src/PeticionCLiente" messageType="tns:PeticionCLienteOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Peticion" createInstance="yes" partnerLink="LinkPeticionCliente" operation="PeticionCLienteOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/BPEL_Tarea4/src/PeticionCLiente" portType="tns:PeticionCLientePortType" variable="PeticionCLienteOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="PeticionCLienteOperationIn" part="nombre"/>
                <to>$PeticionIniIn.parameters/ns0:nombre</to>
            </copy>
        </assign>
        <invoke name="InvocarCliente" partnerLink="LinkCliente" operation="PeticionIni" xmlns:tns="http://ServicioCliente.org/" portType="tns:Service_ClienteSoap" inputVariable="PeticionIniIn" outputVariable="PeticionIniOut"/>
        <assign name="Assign2">
            <copy>
                <from>$PeticionIniOut.parameters/ns0:PeticionIniResult</from>
                <to>$ObtenerPropuestaPilotoIn.parameters/ns1:zona</to>
            </copy>
        </assign>
        <invoke name="InvocarRastreo" partnerLink="LinkRastreo" operation="ObtenerPropuestaPiloto" xmlns:tns="http://ServicioRastreo.org/" portType="tns:Service_RastreoSoap" inputVariable="ObtenerPropuestaPilotoIn" outputVariable="ObtenerPropuestaPilotoOut"/>
        <assign name="Assign3">
            <copy>
                <from>$ObtenerPropuestaPilotoOut.parameters/ns1:ObtenerPropuestaPilotoResult</from>
                <to>$ObtenerConductoresDisponiblesIn.parameters/ns2:placa</to>
            </copy>
        </assign>
        <invoke name="InvocarPiloto" partnerLink="LinkPiloto" operation="ObtenerConductoresDisponibles" xmlns:tns="http://ServicioPiloto.org/" portType="tns:Service_PilotoSoap" inputVariable="ObtenerConductoresDisponiblesIn" outputVariable="ObtenerConductoresDisponiblesOut"/>
        <assign name="Assign4">
            <copy>
                <from>concat(concat(concat('Estimado ', $PeticionCLienteOperationIn.nombre), ' los datos de su viaje son:'), $ObtenerConductoresDisponiblesOut.parameters/ns2:ObtenerConductoresDisponiblesResult)</from>
                <to variable="PeticionCLienteOperationOut" part="respuesta"/>
            </copy>
        </assign>
        <reply name="Respuesta" partnerLink="LinkPeticionCliente" operation="PeticionCLienteOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/BPEL_Tarea4/src/PeticionCLiente" portType="tns:PeticionCLientePortType" variable="PeticionCLienteOperationOut"/>
    </sequence>
</process>
